      <type name="CdnFunctionPolynomialPiece" qualifier="*" ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece" />
    </argument>
  </function>
  <function id="cdn_function_polynomial_piece_equal" name="cdn_function_polynomial_piece_equal">
    <brief>Check whether two function polynomial pieces are equal.</brief>
    <return>
      <doc>`TRUE` if &amp;lt;a&amp;gt; is equal to <ref ref="API Reference::CdnFunctionPolynomialPiece#cdn_function_polynomial_piece_equal::b">cdn_function_polynomial_piece_equal::b</ref>, `FALSE` otherwise.</doc>
      <type builtin="yes" name="gboolean" />
    </return>
    <argument id="cdn_function_polynomial_piece_equal::self" name="self">
      <doc>a <ref ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece">CdnFunctionPolynomialPiece</ref>.</doc>
      <type name="CdnFunctionPolynomialPiece" qualifier="*" ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece" />
    </argument>
    <argument id="cdn_function_polynomial_piece_equal::b" name="b">
      <doc>another <ref ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece">CdnFunctionPolynomialPiece</ref></doc>
      <type name="CdnFunctionPolynomialPiece" qualifier="*" ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece" />
    </argument>
  </function>
  <function id="cdn_function_polynomial_piece_evaluate" name="cdn_function_polynomial_piece_evaluate">
    <brief>Evaluate the piece at <ref ref="API Reference::CdnFunctionPolynomialPiece#cdn_function_polynomial_piece_evaluate::t">cdn_function_polynomial_piece_evaluate::t</ref>.</brief>
    <return>
      <doc>the value of the polynomial.</doc>
      <type builtin="yes" name="gdouble" />
    </return>
    <argument id="cdn_function_polynomial_piece_evaluate::self" name="self">
      <doc>a <ref ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece">CdnFunctionPolynomialPiece</ref>.</doc>
      <type name="CdnFunctionPolynomialPiece" qualifier="*" ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece" />
    </argument>
    <argument id="cdn_function_polynomial_piece_evaluate::t" name="t">
      <doc>at which point to evaluate the piece.</doc>
      <type builtin="yes" name="gdouble" />
    </argument>
  </function>
