      <type name="CdnFunctionPolynomialPiece" qualifier="*" ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece" />
    </argument>
  </function>
  <function id="cdn_function_polynomial_piece_new" name="cdn_function_polynomial_piece_new">
    <brief>Create a new polynomial to be used in a piecewise polynomial function</brief>
    <doc>Create a new polynomial to be used in a piecewise polynomial function. The
coefficients are specified from high to low order.

</doc>
    <return>
      <doc>A <ref ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece">CdnFunctionPolynomialPiece</ref></doc>
      <type name="CdnFunctionPolynomialPiece" qualifier="*" ref="API Reference::CdnFunctionPolynomialPiece#API Reference::CdnFunctionPolynomialPiece" />
    </return>
    <argument id="cdn_function_polynomial_piece_new::begin" name="begin">
      <doc>The polynomial interval begin</doc>
      <type builtin="yes" name="gdouble" />
    </argument>
    <argument id="cdn_function_polynomial_piece_new::end" name="end">
      <doc>The polynomial interval end</doc>
      <type builtin="yes" name="gdouble" />
    </argument>
    <argument id="cdn_function_polynomial_piece_new::coefficients" name="coefficients">
      <doc>The coefficients</doc>
      <type builtin="yes" name="gdouble" qualifier="*" />
    </argument>
    <argument id="cdn_function_polynomial_piece_new::num_coefficients" name="num_coefficients">
      <doc>The number of coefficients provided in <ref ref="API Reference::CdnFunctionPolynomialPiece#cdn_function_polynomial_piece_new::coefficients">cdn_function_polynomial_piece_new::coefficients</ref></doc>
      <type builtin="yes" name="guint" />
    </argument>
  </function>
