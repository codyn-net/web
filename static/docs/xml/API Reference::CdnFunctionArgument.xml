    <return>
      <type builtin="yes" name="void" />
    </return>
    <argument id="cdn_function_argument_get_dimension::self" name="self">
      <doc>a <ref ref="API Reference::CdnFunctionArgument#API Reference::CdnFunctionArgument">CdnFunctionArgument</ref>.</doc>
